{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"lib": [
			"ESNext"
		],
		"types": [
			"@cloudflare/workers-types/experimental"
		],
		"moduleResolution": "bundler",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"verbatimModuleSyntax": true
	},
	"include": [
		"src/**/*.ts"
	]
}
